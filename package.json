{
  "name": "rbac",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "build": "npx tsc",
    "ready": "npm install",
    "start": "node ./build/main.js",
    "dev": "nodemon ./build/main.js",
    "dev:w": "tsc -w",
    "mig:create": "typeorm migration:create ./src/factory/migrations/%npm_config_name%",
    "mig:run": "npx typeorm-ts-node-esm migration:generate ./src/factory/migrations/%npm_config_name% -d ./src/app/config/Datasource.ts",
    "mig:revert": "typeorm migration:revert -- -d ./src/app/config/Datasource.ts",
    "entity:create": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm entity:create -n",
    "migrate:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -n",
    "migrate:run": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run",
    "migrate:revert": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert",
    "schema:drop": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm schema:drop",
    "argument": "echo \"The value of --foo is '%npm_config_foo%'\"",
    "start:dev": "nodemon main.ts"
  },
  "author": {
    "name": "ENJOYS",
    "url": "docs.enjoys.in"
  },
  "license": "ISC",
  "dependencies": {
    "@casl/ability": "^6.7.1",
    "@enjoys/exception": "^1.0.8",
    "@simplewebauthn/server": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "colorette": "^2.0.20",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "googleapis": "^134.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.1",
    "million": "2.6.0-beta.10",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.8",
    "nodemailer-express-handlebars": "^6.1.0",
    "pg": "^8.11.3",
    "rate-limiter-flexible": "^5.0.0",
